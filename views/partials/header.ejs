<div class="navbar navbar-inverse navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <% if((typeof(hide_menubar) == 'undefined') || (hide_menubar == false)){ %>
        <button data-toggle="collapse" data-target=".nav-collapse" class="btn btn-navbar navigation">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      <% } %>

      <a href="/" class="brand">Twitch Bot</a>
      <% if (typeof(hide_menubar) == 'undefined') { %>
      <div class="nav-collapse collapse navigation">
        <ul id="bar-menu-left" class="nav">
          <li class="<%= title === 'Home' ? 'active' : ''  %>">               
            <a href="/">
              <i class="icon-home"> </i>&nbsp;Home
            </a>
          </li>
          <li class="dropdown <%= (title.indexOf('Settings')!==-1 ? 'active' : '') %>">               
            <a href="#" data-toggle="dropdown" role="button" id="dropdown" class="dropdown-toggle">
              <i class="icon-cogs"> </i>&nbsp;Plugins&nbsp;<b class="caret"></b>
            </a>
            <ul role="menu" aria-labelledby="dropdown" class="dropdown-menu">
            <!-- <li>
                <a href="/settings"><i class="icon-cog">&nbsp;</i>Twitch Bot</a>
                </li> -->
            <!-- <li role="presentation" class="divider"></li> -->
            <% plugins.forEach(function(plugin) { %>
                <li>
                  <a href="/plugin/<%=plugin.id%>">
                    <% if (plugin.icon) { %>
                    <i class="<%=plugin.icon%>">&nbsp;</i>
                    <% } %>
                    &nbsp;<%=plugin.name%>
                  </a>
                </li>
            <% }); %>
            </ul>
          </li>
          <!--<li class="<%= title === "Rooms" ? 'active' : ''  %>">               
            <a href="/rooms"><i class="icon-home"> </i>&nbsp;Rooms</a>
          </li>-->
          <li class="<%= title === "Settings" ? 'active' : ''  %>">               
            <a href="/settings"><i class="icon-cog"> </i>&nbsp;Settings</a>
          </li>
        </ul>
        <ul id="bar-menu-right" class="nav pull-right">
          <li><a href="/logout" class="btn-logout"> <i class="icon-signout">&nbsp;</i>Logout</a></li>
        </ul>
      </div>
      <% } %>
    </div>
  </div>
</div>